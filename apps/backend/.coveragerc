[run]
source = .
branch = True
omit = 
    */migrations/*
    */venv/*
    */env/*
    */tests/*
    manage.py
    */settings/*
    */asgi.py
    */wsgi.py
    */urls.py
    */views.py
    pagemaker/integrations/*
    test_jwt.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov
skip_covered = False 